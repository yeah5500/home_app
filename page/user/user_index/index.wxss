/*! Import by _C[1], whose real path we cannot found. */
page {
    --mColor: #4395FF;
    --mColorRgb: 9,187,7;
    --mColorLight: #0bc908;
    --mColorDark: #049203;
    --price: #f66;
    --star: #FBC02D;
    --starGrey: #b6b6b6;
}
/*! Import end */


page {
    font-size: 32rpx;
}

.fb {
    font-weight: bold;
}

.user_content {
    padding: 0 30rpx;
    color: #fff;
    background: var(--mColor);
    position: relative;
    overflow: hidden;
}

.user-elem {
    width: 70rpx;
    height: 70rpx;
    position: absolute;
}

.user-elem:first-child {
    right: -30rpx;
    top: 10%;
}

.user-elem:nth-child(n+2) {
    left: -30rpx;
    bottom: 40%;
    transform: scale(0.8);
}

.Member_head {
    padding: 50rpx 30rpx 60rpx;
    text-align: center;
}

.userinfo-avatar {
    width: 140rpx;
    height: 140rpx;
    border-radius: 50%;
    box-shadow: 0 0 0 6rpx rgba(255,255,255,0.5);
}

.userinfo_name-box {
    margin-top: 30rpx;
    font-size: 36rpx;
}

.userinfo_name {
    max-width: 360rpx;
    display: inline-block;
    vertical-align: bottom;
}

.tongbu-btn {
    display: inline-block;
    vertical-align: bottom;
    margin-left: 20rpx;
    font-size: 1.1em;
}

.vip-box {
    padding: 20rpx 30rpx;
    background: var(--mColorLight);
    color: var(--mColorDark);
    border-radius: 12rpx 12rpx 0 0;
    background-image: linear-gradient(120deg,rgba(255,255,255,.7),rgba(255,255,255,.4));
    position: relative;
    box-shadow: 0 -5rpx 45rpx 0 rgba(0,0,0,.06),1rpx 1rpx 0 rgba(255,255,255,.2) inset;
}

.open-member {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.open-member-txt,.vip-balance {
    font-weight: bold;
}

.open-member-txt .iconfont {
    font-size: 36rpx;
    margin-right: 10rpx;
}

.open-member-btn {
    background: transparent;
    color: inherit;
    font-size: 12px;
    border: 1px solid;
    line-height: 26px;
    padding: 0 20rpx;
    border-radius: 6rpx;
}

.open-member-btn::after {
    border: none;
}

.open-member-pop {
    z-index: 2;
}

.pop-cont {
    width: 600rpx;
    padding: 60rpx;
    background: #fff;
    border-radius: 10rpx;
    z-index: 1;
}

.recharge-colse {
    position: absolute;
    right: 0;
    top: 0;
    background: var(--mColorLight);
    width: 78rpx;
    height: 78rpx;
    border-bottom-left-radius: 80%;
}

.recharge-colse::before,.recharge-colse::after {
    content: '';
    display: block;
    width: 50%;
    height: 1px;
    background: #fff;
    position: absolute;
    right: 20%;
    top: 45%;
    transform: rotate(45deg);
}

.recharge-colse::after {
    transform: rotate(-45deg);
}

.recharge-colse:activeP {
    opacity: 0.8;
}

.pop-tel-inp {
    margin-top: 20rpx;
}

.pop-tel-inp,.pop-tel-check {
    height: 72rpx;
    padding: 0 30rpx;
    border: 1px solid var(--mColor);
    border-radius: 6rpx;
    font-size: 14px;
}

.get-code {
    display: inline-block;
    background: var(--mColor);
    color: #fff;
    font-size: 28rpx;
    border-radius: 6rpx;
}

.get-code:active,.pop-sure-btn:active {
    opacity: 0.8;
}

.get-code::after {
    border: none;
}

.pop-sure-btn {
    background: var(--mColor);
    color: #fff;
    font-size: 32rpx;
    border-radius: 6rpx;
    margin-top: 50rpx;
}

.pop-sure-btn::after {
    border: none;
}

.get-weixintel {
    color: var(--mColor);
    background: transparent;
    font-size: 36rpx;
    margin-top: 20rpx;
    border-radius: 6rpx;
    line-height: 78rpx;
}

.get-weixintel::after {
    border-color: currentColor;
    border-radius: 6rpx;
}

.my-menu {
    box-shadow: 0 -8rpx 20rpx 0 rgba(0,0,0,0.05);
    position: relative;
}

.my-menu-box {
    background: #fff;
    margin-bottom: 20rpx;
   
}

.my-menu-item {
    padding: 26rpx 30rpx;
    font-weight: bold;
    color: #505050;
}

.wechat-setting {
    font-weight: bold;
    color: #505050;
    text-align: left;
    border: none;
}

button.authorization-btn::after {
    display: none;
}

.authorization-btn {
    padding: 6rpx 30rpx;
    border-radius: 0;
    background: #fff;
    font-size: 32rpx;
}

.authorization-btn .iconfont {
    font-size: 38rpx;
}

.my-menu-item .iconfont {
    font-weight: normal;
    display: inline-block;
    transform: scale(1.2);
}

.arrowL {
    display: block;
    padding-left: 30px;
}

.arrowRv::before {
    transform: translateY(-50%) scale(0.7) rotate(135deg);
}

.forum-box {
    overflow: hidden;
}

.my-menu-subitem {
    padding: 12px 20px 12px 10px;
    color: #666;
}

.my-menu-subitem:nth-child(n+2)::before {
    left: 40rpx;
}

.my-menu-subitem:active {
    background: rgba(0,0,0,0.05);
}

.menu-line {
    text-align: center;
    display: flex;
    line-height: 1.1;
}

.menu-line .my-menu-subitem {
    flex: 1;
    padding: 20rpx 0;
    font-size: 26rpx;
    color: #666;
}

.menu-line .iconfont {
    font-size: 52rpx;
    margin-bottom: 20rpx;
    display: inline-block;
}

.menu-line .iconfont {
    font-size: 50rpx;
    margin-bottom: 20rpx;
    display: inline-block;
}

.copyright {
    text-align: center;
    line-height: 40px;
    height: 40px;
    margin-bottom: 10px;
}

.copyright-text {
    width: 100%;
    font-size: 28rpx;
}

.copyright-text::after {
    display: inline-block;
    border: 5px solid;
    border-color: transparent transparent transparent #ccc;
    content: "";
    margin-left: 10rpx;
}

.links .my-menu-item:active {
    background: rgba(0,0,0,0.05);
}

.loading-icon {
    width: 72px;
    height: 72px;
}

.loading-icon+text {
    display: block;
}

page .iconfont {
    font-size: inherit;
}

page .iconfont::before {
    font-weight: normal!important;
}

.navigator-hover {
    opacity: 1;
    background: none;
}

.zanwu {
    color: lightgrey;
    padding: 80rpx;
    text-align: center;
}

.zanwu text.iconfont {
    font-size: 260rpx;
    line-height: 110%;
}
/*! Import end */
.my-order {
    background: #fff;
    border-radius: 10%;
}

.my-order-tit {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 30rpx;

    border-radius: 10%;
}

.right-more {
    font-size: 0.8em;
    color: #999;
}

.right-more.arrowR {
    padding-right: 24rpx;
}

.right-more.arrowR::before {
    right: 0;
}

.orderBox {
    justify-content: space-between;
}

.orderBox .sub {
    padding: 30rpx 0;
    position: relative;
}

.orderBox .sub:active {
    background-color: rgba(0,0,0,0.08);
}

.orderBox .iconfont {
    font-size: 48rpx;
    margin-bottom: 20rpx;
    color: #666;
}

.order-state-num {
    background: #fff;
    color: #f66;
    border: 2rpx solid;
    width: 40rpx;
    height: 40rpx;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
    font-size: 12px;
    font-weight: bold;
    position: absolute;
    top: 20rpx;
    right: 24rpx;
}

.subfont {
    line-height: 30rpx;
    color: #666;
    font-size: 30rpx;
}

.copyright-img {
    width: 500rpx;
    height: 150rpx;
    margin: 50rpx auto;
}

.ts-block {
    float: right;
}

.gray {
    color: #ccc;
    font-size: 12px;
}

.redcirle {
    display: inline-block;
    width: 20rpx;
    height: 20rpx;
    background: #f00;
    border-radius: 40rpx;
    margin-bottom: 0rpx;
    margin-right: 10rpx;
}