.modal {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 630rpx;
    animation: ruleAni 300ms linear forwards;
}

@-webkit-keyframes ruleAni {
    0% {
        transform: translate(-50%,-50%) scale(0.3,0.3);
    }

    80% {
        transform: translate(-50%,-50%) scale(1.2,1.2);
    }

    100% {
        transform: translate(-50%,-50%) scale(1,1);
    }
}

@keyframes ruleAni {
    0% {
        transform: translate(-50%,-50%) scale(0.3,0.3);
    }

    80% {
        transform: translate(-50%,-50%) scale(1.2,1.2);
    }

    100% {
        transform: translate(-50%,-50%) scale(1,1);
    }
}

